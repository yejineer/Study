package practice20;

public enum Language {
	LAN_KOR(new String[] {"가위", "바위", "보"}), LAG_ENG(new String[] {"SCISSOR", "바위", "보"}), LAG_JAP(new Japanese());

	Language(String[] var) { 	// Constructor
		LanguageVar = var;
	}
	
	String[] LanguageVar;
	public String[] getLanguageVar() {
		return LanguageVar;
	}
	
}