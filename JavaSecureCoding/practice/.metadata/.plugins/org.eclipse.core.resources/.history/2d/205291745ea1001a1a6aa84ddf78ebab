// 컴퓨터학과 20170964 이예진
package practice19;

public enum Score {
	WIN, LOSE, EQUAL;

	public static String print(Score rslt) {
		switch (rslt.name()) {
			case "WIN":
				return "당신이 이겼습니다.";
			case "LOSE":
				return "컴퓨터가 이겼습니다.";
			case "EQUAL":
				return "비겼습니다.";
			default:
				return null;
		}
	}
}
