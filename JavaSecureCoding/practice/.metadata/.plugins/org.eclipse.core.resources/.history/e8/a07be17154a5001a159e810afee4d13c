// 컴퓨터학과 20170964 이예진
package practice20;

public enum Game {

	ROCK(0), PAPER(1), SCISSORS(2), ERROR(-1);
	
	Game(int num) { // Constructor
		GameNum = num;
	}
	
	int GameNum;
	public int getGameNum() {
		return GameNum;
	}
	
	public static Game encode(String input) {
		switch (input) {
			case "바위":
				return ROCK;
			case "보":
				return PAPER;
			case "가위":
				return SCISSORS;
			default:
				return ERROR;
		}
	}
	
}
